@article{reflections30years,
  author   = {Malkiel, Burton G.},
  title    = {Reflections on the Efficient Market Hypothesis: 30 Years Later},
  journal  = {Financial Review},
  volume   = {40},
  number   = {1},
  pages    = {1-9},
  keywords = {efficient markets, stock market predictability, G12, G14},
  doi      = {https://doi.org/10.1111/j.0732-8516.2005.00090.x},
